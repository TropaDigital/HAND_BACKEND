{
  "name": "haand-bid",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "scripts": {
    "prepare": "husky install && prisma generate",
    "build": "tsc && cp src/api.schema.yml dist/src/api.schema.yml",
    "start": "node dist/src/index.js",
    "start:dev": "dotenv -e .env.development -- ts-node-dev src/index.ts",
    "test": "dotenv -e .env.test -- jest --clearCache && jest --passWithNoTests --runInBand",
    "test:unit": "npm run test -- --watch",
    "test:unit:staged": "npm run test -- --findRelatedTests",
    "test:coverage": "npm run test -- --coverage",
    "test:mutation": "stryker run",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "lint:deadcode": "ts-prune -e",
    "commit": "git-cz"
  },
  "contributors": [
    {
      "name": "Bruno Afonso",
      "email": "brunohafonso@gmail.com"
    },
    {
      "name": "Lucas Sim√£o",
      "email": "lucas64_64@hotmail.com"
    }
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haandsolutions/haand-bib.git"
  },
  "bugs": {
    "url": "https://github.com/haandsolutions/haand-bib/issues"
  },
  "homepage": "https://github.com/haandsolutions/haand-bib#readme",
  "description": "webservice to provide the features of ases system",
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "got": "11",
    "helmet": "^5.0.2",
    "http-status-code": "^2.1.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "morgan": "^1.10.0",
    "openapi-comment-parser": "^1.0.0",
    "pino": "^7.9.2",
    "pino-pretty": "^7.6.0",
    "prisma": "^3.11.1",
    "prom-client": "^14.0.1",
    "swagger-stats": "^0.99.2",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@stryker-mutator/core": "^5.5.1",
    "@stryker-mutator/jest-runner": "^5.5.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^27.0.3",
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.3",
    "@types/pino": "^7.0.5",
    "@types/swagger-stats": "^0.95.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "ts-prune": "^0.10.3",
    "typescript": "^4.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "feat": {
        "description": "A new feature",
        "title": "Features"
      },
      "fix": {
        "description": "A bug fix",
        "title": "Bug Fixes"
      },
      "docs": {
        "description": "Documentation only changes",
        "title": "Documentation"
      },
      "style": {
        "description": "Changes that do not affect the meaning of the code (white-space, formatting, missing semi-colons, etc)",
        "title": "Styles"
      },
      "refactor": {
        "description": "A code change that neither fixes a bug nor adds a feature",
        "title": "Code Refactoring"
      },
      "perf": {
        "description": "A code change that improves performance",
        "title": "Performance Improvements"
      },
      "test": {
        "description": "Adding missing tests or correcting existing tests",
        "title": "Tests"
      },
      "build": {
        "description": "Changes that affect the build system or external dependencies (example scopes: gulp, broccoli, npm)",
        "title": "Builds"
      },
      "ci": {
        "description": "Changes to our CI configuration files and scripts (example scopes: Travis, Circle, BrowserStack, SauceLabs)",
        "title": "Continuous Integrations"
      },
      "chore": {
        "description": "Other changes that don't modify src or test files",
        "title": "Chores"
      },
      "revert": {
        "description": "Reverts a previous commit",
        "title": "Reverts"
      },
      "improvement": {
        "description": "improvement applyed to the project",
        "title": "improvement"
      }
    }
  }
}
